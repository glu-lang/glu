func printCharacter(c: Char) {
    std::print(c);
}

func main() -> Int {
    // CHECK: invalid_char_lit.glu:11:20: error: Character literal must be a single character
    printCharacter("abc");
    // CHECK: invalid_char_lit.glu:13:20: error: Character literal must be a single character
    printCharacter("");
    return 0;
}

