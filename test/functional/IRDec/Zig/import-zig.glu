import simple;

@no_mangling @c_variadic func printf(s: *Char) -> Int;

func main() {
    // CHECK: Hello, World!
    simple::sayHello();
    // CHECK: Pizza count is 81, compiler is Zig
    printf("Pizza count is %d, compiler is %s\n", simple::square(9), simple::getCC());
}
