// RUN: mkdir -p %t
// RUN: zig build-obj -fllvm -fno-strip %S/simple.zig -femit-llvm-ir=%t/simple.ll
// RUN: gluc %s -I %t -o %t/import-zig && %t/import-zig | FileCheck -v %s
