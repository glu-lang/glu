//
// RUN: mkdir -p %t
// RUN: gluc %s --linker clang++ -o %t/auto-import-c-cpp && %t/auto-import-c-cpp | FileCheck -v %s
//

import cmod;
import cppmod;

func main() {
    // CHECK: C count is 7, C++ count is 11, compilers are Clang and Clang++
    std::printf(
        "C count is %d, C++ count is %d, compilers are %s and %s\n",
        cmod::getCCount(),
        cppmod::getCppCount(),
        cmod::getCCompiler(),
        cppmod::getCppCompiler()
    );
}
