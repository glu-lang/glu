//
// RUN: gluc %s --linker clang++ -o %t && %t | FileCheck -v %s
//

import cmod;
import cppmod;
import rustmod::rustmod;

func main() {
    // CHECK: C count is 7, C++ count is 11, compilers are Clang and Clang++
    std::printf(
        "C count is %d, C++ count is %d, compilers are %s and %s\n",
        cmod::getCCount(),
        cppmod::getCppCount(),
        cmod::getCCompiler(),
        cppmod::getCppCompiler()
    );
    // CHECK: Rust count is 13, compiler is Rust
    std::printf(
        "Rust count is %d, compiler is %.*s\n",
        rustmod::get_rust_count(),
        rustmod::get_rust_compiler().length,
        rustmod::get_rust_compiler().data_ptr
    );
}
