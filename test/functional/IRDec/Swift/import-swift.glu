import simple::simple;

@no_mangling @c_variadic func printf(s: *Char) -> Int;

func main() {
    // CHECK: Square of 9 is 81
    printf("Square of 9 is %d\n", simple::square({9}));
    // CHECK: Pizza count is 42
    printf("Pizza count is %d\n", simple::getPizzaCount());
    // CHECK: CC is Swift
    printf("CC is ");
    simple::printCC();
    printf("\n");
}
