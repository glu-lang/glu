func testWhile() -> Void {
    var outer = 1;

    while outer < 5 {
        var inner = outer * 2;
        while inner > 0 {
            std::printout(".");
            inner = inner - 1;
        }
        std::print("");
        outer = outer + 1;
    }
}

// CHECK: ..
// CHECK: ....
// CHECK: ......
// CHECK: ........
func main() -> Int {
    testWhile();
    return 0;
}
