//
// RUN: gluc %s -o %t && %t | FileCheck -v %s
//

func retTrue() -> Bool {
	std::print("retTrue called");
	return true;
}

func retFalse() -> Bool {
	std::print("retFalse called");
	return false;
}

func main() -> Int {
	// CHECK: Testing AND:
	std::print("Testing AND:");
	// CHECK-NEXT: retTrue called
	// CHECK-NEXT: retFalse called
	// CHECK-NEXT: AND result: false
	if retTrue() && retFalse() && retTrue() {
		std::print("AND result: true");
	} else {
		std::print("AND result: false");
	}

	std::print("Testing OR:");
	// CHECK: Testing OR:
	// CHECK-NEXT: retFalse called
	// CHECK-NEXT: retTrue called
	// CHECK-NEXT: OR result: true
	if retFalse() || retTrue() || retTrue() {
		std::print("OR result: true");
	} else {
		std::print("OR result: false");
	}

	return 0;
}
