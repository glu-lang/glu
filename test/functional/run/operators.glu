//
// RUN: gluc %s -o %t && %t | FileCheck -v %s
//

func main() -> Int {
	let a: Int = 4;
	let b: Int = 1;

	let sum = a + b;
	// CHECK: sum: 5
	std::printout("sum: ");
	std::print(sum);

	let diff = a - b;
	// CHECK: diff: 3
	std::printout("diff: ");
	std::print(diff);

	let div = a / b;
	// CHECK: div: 4
	std::printout("div: ");
	std::print(div);

	let mod = a % b;
	// CHECK: mod: 0
	std::printout("mod: ");
	std::print(mod);

	let mod2 = b % a;
	// CHECK: mod: 1
	std::printout("mod: ");
	std::print(mod2);

	let eq1 = a == b;
	// CHECK: eq1: false
	std::printout("eq1: ");
	std::print(eq1);

	let eq2 = a == a;
	// CHECK: eq2: true
	std::printout("eq2: ");
	std::print(eq2);

	let neq1 = a != b;
	// CHECK: neq1: true
	std::printout("neq1: ");
	std::print(neq1);

	let neq2 = a != a;
	// CHECK: neq2: false
	std::printout("neq2: ");
	std::print(neq2);

	let lt1 = a < b;
	// CHECK: lt1: false
	std::printout("lt1: ");
	std::print(lt1);

	let lt2 = b < a;
	// CHECK: lt2: true
	std::printout("lt2: ");
	std::print(lt2);

	let gt1 = a > b;
	// CHECK: gt1: true
	std::printout("gt1: ");
	std::print(gt1);

	let gt2 = b > a;
	// CHECK: gt2: false
	std::printout("gt2: ");
	std::print(gt2);

	let le1 = a <= b;
	// CHECK: le1: false
	std::printout("le1: ");
	std::print(le1);

	let le2 = b <= a;
	// CHECK: le2: true
	std::printout("le2: ");
	std::print(le2);

	let le3 = a <= a;
	// CHECK: le3: true
	std::printout("le3: ");
	std::print(le3);

	let ge1 = a >= b;
	// CHECK: ge1: true
	std::printout("ge1: ");
	std::print(ge1);

	let ge2 = b >= a;
	// CHECK: ge2: false
	std::printout("ge2: ");
	std::print(ge2);

	let ge3 = a >= a;
	// CHECK: ge3: true
	std::printout("ge3: ");
	std::print(ge3);

	let and = a & b;
	// CHECK: and: 0
	std::printout("and: ");
	std::print(and);

	let or = a | b;
	// CHECK: or: 5
	std::printout("or: ");
	std::print(or);

	let xor = a ^ b;
	// CHECK: xor: 5
	std::printout("xor: ");
	std::print(xor);

	let shl = a << b;
	// CHECK: shl: 8
	std::printout("shl: ");
	std::print(shl);

	let shr = a >> b;
	// CHECK: shr: 2
	std::printout("shr: ");
	std::print(shr);

	let compl = -~a;
	// CHECK: compl: 5
	std::printout("compl: ");
	std::print(compl);

	let noop = +a;
	// CHECK: noop: 4
	std::printout("noop: ");
	std::print(noop);
    return 0;
}
